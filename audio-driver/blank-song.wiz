

import "data-formats";

config {
    trim = true
}

bank rodata @ 0x0000 : [constdata;  0x40];

in rodata {

const BlankSong = SongHeader{
    channels = [ MusicChannelHeader{ starting = 0xffff, loop = 0xffff } ; N_MUSIC_CHANNELS ],

    // Maximum tick timer
    tickTimer = 255,

    nSubroutines = 0,
};

}

